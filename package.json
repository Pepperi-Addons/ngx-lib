{
  "name": "ngx-lib-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "eslint \"**/*.{ts,tsx}\" ",
    "lint-fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "e2e": "ng e2e",
    "build-lib": "ng build ngx-lib",
    "build-lib-watch": "ng build ngx-lib --watch",
    "build-lib-prod": "ng build ngx-lib --configuration production",
    "publish-lib": "cd dist && cd ngx-lib && npm publish",
    "publish-lib-beta": "cd dist && cd ngx-lib && npm publish --tag beta",
    "pack-lib": "npm run build-lib-prod && cd dist && cd ngx-lib && npm pack",
    "generate-icons": "svg-to-ts",
    "compile": "npm run build-lib-prod && ng build",
    "prepare": "npm run compile && npm run lint",
    "prepare-no-lint": "npm run compile",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -s .storybook/public -p 6006 --no-manager-cache",
    "build-storybook": "npm run docs:json && build-storybook -s .storybook/public",
    "deploy-storybook": "storybook-to-ghpages --out=./dist/storybook --s build-storybook",
    "chromatic": "chromatic --exit-zero-on-changes --project-token=CHROMATIC_PROJECT_TOKEN"
  },
  "svg-to-ts": [
    {
      "conversionType": "constants",
      "srcFiles": [
        "./projects/ngx-lib/icon/svgs/**/*.svg"
      ],
      "outputDirectory": "./projects/ngx-lib/icon",
      "interfaceName": "IPepIconData",
      "typeName": "PepIconType",
      "prefix": "pepIcon",
      "svgoConfig": {
        "plugins": [
          {
            "cleanupAttrs": true
          }
        ]
      },
      "fileName": "icon-generated.model",
      "additionalModelFile": "./projects/ngx-lib/icon",
      "compileSources": true
    },
    {
      "conversionType": "object",
      "srcFiles": [
        "./projects/ngx-lib/icon/svgs/**/*.svg"
      ],
      "outputDirectory": "./projects/ngx-lib/icon",
      "svgoConfig": {
        "plugins": [
          {
            "cleanupAttrs": true
          }
        ]
      },
      "fileName": "icon-generated-all.model",
      "delimiter": "SNAKE",
      "objectName": "allIcons",
      "additionalModelFile": "./projects/ngx-lib/icon",
      "compileSources": true
    }
  ],
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^12.5.3",
    "@angular/animations": "~12.1.1",
    "@angular/cdk": "^12.0.1",
    "@angular/common": "~12.1.1",
    "@angular/compiler": "~12.1.1",
    "@angular/core": "~12.1.1",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "~12.1.1",
    "@angular/material": "^12.0.1",
    "@angular/material-moment-adapter": "^12.1.1",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/router": "~12.1.1",
    "@mat-datetimepicker/core": "^6.0.3",
    "@mat-datetimepicker/moment": "^6.0.3",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@pepperi-addons/papi-sdk": "^1.29.5",
    "@tweenjs/tween.js": "^17.1.1",
    "hammerjs": "^2.0.8",
    "mini-css-extract-plugin": "^1.6.0",
    "moment": "^2.29.1",
    "ng-gallery": "^5.0.0",
    "ng2-file-upload": "^1.4.0",
    "ngx-quill": "^14.0.0",
    "ngx-signaturepad": "0.0.9",
    "ngx-translate-multi-http-loader": "^3.0.0",
    "ngx-virtual-scroller": "^4.0.3",
    "quill": "^1.3.7",
    "rxjs": "^6.5.5",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.1",
    "@angular/cli": "~12.1.1",
    "@angular/compiler-cli": "~12.1.1",
    "@angular/elements": "^12.0.1",
    "@babel/core": "^7.14.3",
    "@compodoc/compodoc": "^1.1.18",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/angular": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/preset-scss": "^1.0.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@webcomponents/custom-elements": "^1.4.3",
    "angular-cli-ghpages": "^1.0.0-rc.1",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.8.3",
    "codelyzer": "^6.0.0",
    "eslint": "^7.27.0",
    "eslint-plugin-storybook": "^0.5.10",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.0",
    "protractor": "~7.0.0",
    "svg-to-ts": "^5.0.1",
    "ts-node": "~8.3.0",
    "typescript": "~4.2.4",
    "webpack-dev-server": "^4.6.0"
  }
}
