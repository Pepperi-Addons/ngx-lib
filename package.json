{
    "name": "pep-angular-library",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build-lib": "ng build ngx-lib",
        "build-lib-watch": "ng build ngx-lib --watch",
        "build-lib-prod": "ng build ngx-lib --prod",
        "link-lib": "cd dist && cd ngx-lib && npm link",
        "link+install-lib": "npm run link-lib && npm i file:./dist/ngx-lib",
        "pack-lib": "cd dist && cd ngx-lib && npm pack",
        "publish-lib": "cd dist && cd ngx-lib && npm publish",
        "pack+install-lib": "npm run pack-lib && npm i",
        "build+pack+install-lib": "npm run build-lib && npm run pack-lib && npm i",
        "generate-icons": "svg-to-ts",
        "analyze": "ng build web-app --prod --stats-json && webpack-bundle-analyzer ./dist/web-app/stats.json",
        "webpack": "webpack"
    },
    "svg-to-ts": {
        "conversionType": "constants",
        "srcFiles": [
            "./projects/ngx-lib/icon/svgs/**/*.svg"
        ],
        "outputDirectory": "./projects/ngx-lib/icon",
        "interfaceName": "PepIcon",
        "typeName": "PepIconType",
        "prefix": "pepIcon",
        "svgoConfig": {
            "plugins": [
                {
                    "cleanupAttrs": true
                }
            ]
        },
        "fileName": "icon.model",
        "additionalModelFile": "./projects/ngx-lib/icon",
        "compileSources": true
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~10.0.6",
        "@angular/cdk": "^10.0.1",
        "@angular/common": "~10.0.6",
        "@angular/compiler": "~10.0.6",
        "@angular/core": "~10.0.6",
        "@angular/flex-layout": "^10.0.0-beta.32",
        "@angular/forms": "~10.0.6",
        "@angular/material": "^10.1.3",
        "@angular/material-moment-adapter": "^10.1.3",
        "@angular/platform-browser": "~10.0.6",
        "@angular/platform-browser-dynamic": "~10.0.6",
        "@angular/router": "~10.0.6",
        "@hallysonh/ngx-imageviewer": "^2.0.4",
        "@mat-datetimepicker/core": "^5.0.1",
        "@mat-datetimepicker/moment": "^5.0.1",
        "@ngx-gallery/core": "^5.0.0-beta.0",
        "@ngx-translate/core": "^13.0.0",
        "@ngx-translate/http-loader": "^6.0.0",
        "@tweenjs/tween.js": "^17.1.1",
        "angular2-signaturepad": "^2.11.0",
        "hammerjs": "^2.0.8",
        "moment": "^2.26.0",
        "ng2-file-upload": "^1.4.0",
        "ngx-quill": "^12.0.1",
        "ngx-translate-multi-http-loader": "^3.0.0",
        "quill": "^1.3.7",
        "rxjs": "^6.5.5",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1000.7",
        "@angular-devkit/build-ng-packagr": "~0.1000.7",
        "@angular/cli": "~10.0.5",
        "@angular/compiler-cli": "~10.0.6",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "codelyzer": "^6.0.0-next.1",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~3.3.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ng-packagr": "^10.0.3",
        "ngx-library-assets": "^1.2.0",
        "protractor": "~7.0.0",
        "svg-to-ts": "^5.0.1",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~3.9.5",
        "webpack": "^4.44.1",
        "webpack-bundle-analyzer": "^3.8.0",
        "webpack-cli": "^3.3.12"
    }
}